<%= form_for @company do |f| %>
    <%= f.label :name  %>
    <%= f.text_field :name  %> <br>


    <%= f.fields_for :buildings, Building.all do |bf|  %>
        <div>
            <%= bf.object.name %> <br>
            <%= bf.collection_check_boxes :offices, bf.object.get_available_offices, :floor, :floor %> <br>
        </div>
    <% end %>

    <%= f.submit %>

<% end  %>
